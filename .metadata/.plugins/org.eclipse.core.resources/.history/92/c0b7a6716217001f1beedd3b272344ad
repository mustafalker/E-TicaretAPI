package com.example.ETicaretAPI.RestApi;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

import com.example.ETicaretAPI.BLL.IETicaretCategoryServiceBLL;

@RestController
@RequestMapping("/api");
public class ETicaretCategoriesController {
	private final IETicaretCategoryServiceBLL categoryServiceBLL;
	@Autowired
	public ETicaretCategoriesController(IETicaretCategoryServiceBLL categoryServiceBLL) {
		this.categoryServiceBLL = categoryServiceBLL;
	}
	@GetMapping("/categories");
}
